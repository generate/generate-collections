{
  "name": "generate-collections",
  "description": "Add the docs, includes, badges, and layouts collections, along with a few generic defaults to your generator.",
  "version": "0.2.1",
  "homepage": "https://github.com/generate/generate-collections",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-collections",
  "bugs": {
    "url": "https://github.com/jonschlinkert/generate-collections/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "falsey": "^0.3.0",
    "is-valid-app": "^0.1.1",
    "isobject": "^2.0.0"
  },
  "devDependencies": {
    "engine-base": "^0.1.2",
    "generate": "^0.4.14",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "^0.1.7",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.2.0",
    "mocha": "^2.4.5"
  },
  "keywords": [
    "collections",
    "generate",
    "generateplugin",
    "generator",
    "plugin",
    "template",
    "templates",
    "verb",
    "verb-generator",
    "verbgenerator"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default-dev",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "generate-license",
        "generate-mocha",
        "generate-node"
      ],
      "highlight": "generate-file"
    },
    "reflinks": [
      "assemble",
      "common-middleware",
      "generate",
      "update",
      "verb",
      "verb-readme-generator",
      "base"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
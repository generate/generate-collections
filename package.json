{
  "name": "generate-collections",
  "description": "Use this generator to add some common default settings to your 'generate', verb, and assemble generators.",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/generate-collections",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/generate-collections",
  "bugs": {
    "url": "https://github.com/jonschlinkert/generate-collections/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "isobject": "^2.0.0",
    "lazy-cache": "^1.0.3"
  },
  "devDependencies": {
    "generate": "^0.4.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "*",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.2.0",
    "mocha": "*"
  },
  "keywords": [
    "generate",
    "generator",
    "template",
    "templates",
    "verb",
    "verb-generator",
    "verbgenerator"
  ],
  "verb": {
    "related": {
      "list": [
        "assemble",
        "generate",
        "verb"
      ],
      "highlight": "generate"
    },
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "assemble",
      "common-middleware",
      "generate",
      "verb"
    ]
  }
}
